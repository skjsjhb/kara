{
  "name": "@skjsjhb/kara",
  "main": "kara.d.ts",
  "bin": {
    "kara": "bin/kara.js"
  },
  "version": "0.2.1",
  "exports": [
    "./kara.d.ts",
    "./remote.d.ts"
  ],
  "description": "UI in browser.",
  "scripts": {
    "prepublish": "pnpm build",
    "install": "node setup.mjs",
    "build": "node build.mjs",
    "makebin": "cd build && npx pkg -t node18 main.js -o kara.exe",
    "binary:windows": "cd build && tar.exe -acf kara.zip client.js karac.exe kara.exe",
    "binary:unix": "cd build && tar -zcf kara.tar.gz client.js karac kara"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [],
  "author": "skjsjhb",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/skjsjhb/kara.git"
  },
  "dependencies": {
    "get-port": "^7.0.0",
    "nanoid": "^5.0.4",
    "serve-handler": "^6.1.5",
    "tiny-emitter": "^2.1.0",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "global-agent": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.3",
    "@types/serve-handler": "^6.1.4",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "esbuild": "^0.19.8",
    "pkg": "^5.8.1",
    "typescript": "^5.3.3",
    "upx": "^1.0.6"
  }
}
